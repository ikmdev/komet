               </VBox.margin>
               <content>
                  <ListView fx:id="patternInstancesListView" maxHeight="426.0" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" styleClass="pattern-instances-list-view" />
               </content>
            </TitledPane>
         </children>
      </VBox>
   </children>
   <opaqueInsets>
      <Insets bottom="4.0" />
   </opaqueInsets>
</HBox>
